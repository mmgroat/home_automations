{
  "ha_basepath": "http://<your_local_ip>:<your_local_port>/api/",
  "light_on_api": "services/light/turn_on",
  "light_off_api": "services/light/turn_off",
  "ha_token": "<your_home_assistant_token>",
  "processes": [
    {
      "processname": "Zoom",
      "nocallprocesscount": 6
    }
  ],
  "entities": ["light.mike_s_light", "light.living_room_light_four", <your_lights>],
  "alert_color": [255, 25, 255],
  "alert_color_hs": [299.055, 90.157],
  "alert_color_mismatch": [224, 0, 192],
  "alert_brightness": 255,
  "default_temperature": 4000,
  "default_brightness": 51,
  "alert_color_error_range": 40,
  "rest_API_delay_ms": 1000
}
